<div class="main">
  <div id="video-background">
    <%= video_tag "travelize.mp4", autoplay:true, width: "100%", loop: true, muted: true%>
    <div class="mainSearch">
      <%= form_tag("/list_venues", method: "post",class: "form") do %>
      <div class="where">
        <p id="locationTag">Where</p>
        <%= search_field_tag :location, '', id: 'location' , placeholder: "Destination, city, address"%>
      </div>
      <div class="what">
        <p id="interestTag">What</p>
        <%= select_tag "interest", options_for_select(["Restaurants", "Shopping", "Art", "History", "LGBTQ", "Nightlife", "Sports", "Pubs & Breweries", "Nature", "Music"]), id: 'interest', placeholder: "Categories" %>
      </div>
      <div class="searchContainer">
        <div class="searchBtn">
          <%= submit_tag("Search", :class => 'searchBtn') %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>




